{
  "extends": [
    "tslint:recommended",
    "tslint-plugin-prettier"
  ],
  "rules": {
    "array-type": false,
    "prettier": true,
    "quotemark": false,
    "ordered-imports": false,
    "interface-name": false,
    "object-literal-sort-keys": false,
    "interface-over-type-literal": false,
    "no-console": {
      "severity": "warning"
    }
  }
}